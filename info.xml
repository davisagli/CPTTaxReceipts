<?xml version="1.0"?>
<extension key="org.cpt.cpttaxreceipts" type="module">
  <file>cpttaxreceipts</file>
  <name>CPT Tax Receipts</name>
  <description>
      Creates annual PDF tax receipts including a table of contributions.
  </description>
  <license>AGPL</license>
  <maintainer>
    <author>David Glick</author>
    <email>david@glicksoftware.com</email>
  </maintainer>
  <releaseDate>2014-12-18</releaseDate>
  <version>1.0</version>
  <compatibility>
    <ver>4.4</ver>
  </compatibility>
  <urls>
    <url desc="documentation">https://github.com/davisagli/cpttaxreceipts/blob/master/README.md</url>
  </urls>
  <comments>
      Based on the CDN Tax Receipts extension (https://github.com/jake-mw/CDNTaxReceipts)
  </comments>
  <civix>
    <namespace>CRM/CPTTaxReceipts</namespace>
  </civix>
</extension>
